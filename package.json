{
  "name": "large",
  "version": "1.0.0",
  "description": "A place for your big ideas",
  "private": true,
  "scripts": {
    "clean": "pnpm -r exec rimraf dist node_modules",
    "start": "dotenvx run -- turbo start",
    "build": "dotenvx run -- turbo run build",
    "dev": "dotenvx run -- turbo run dev",
    "db:push": "dotenvx run -- turbo run db:push",
    "db:generate": "turbo run db:generate",
    "lint": "turbo run lint",
    "format": "turbo run format",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.49.0",
    "rimraf": "^6.0.1",
    "turbo": "^2.5.6",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@10.15.1",
  "engines": {
    "node": ">=20.0.0"
  }
}
